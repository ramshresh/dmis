{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Application Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "application template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "kartik-v/dependent-dropdown": "dev-master",
    "kartik-v/yii2-builder": "dev-master",
    "kartik-v/yii2-dropdown-x": "*",
    "kartik-v/yii2-widget-depdrop": "*",
    "kartik-v/yii2-date-range": "*",
    "kartik-v/yii2-widgets": "*",
    "ramshresh/yii2-geo": "dev-master",
    "yiisoft/yii2-jui": "*",
    "mdmsoft/yii2-widgets": "~1.0",
    "c006/yii2-migration-utility": "dev-master",
    "miloschuman/yii2-highcharts-widget": "dev-master",
    "ramshresh/yii2-ar-behaviors": "dev-master",
    "ramshresh/yii2-gallery-manager": "dev-master",
    "yiisoft/yii2-authclient": "~2.0",
    "kartik-v/yii2-editable": "*",
    "ramshresh/yii2-assets-adminlte": "dev-master",
    "sintret/yii2-chat-adminlte": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ramshresh/openlayers2-pointpicker.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ramshresh/yii2-geo.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ramshresh/yii2-ar-behaviors.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ramshresh/yii2-gallery-manager.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ramshresh/yii2-assets-adminlte.git"
    }
  ]
}
